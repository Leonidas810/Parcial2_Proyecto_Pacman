import greenfoot.*;  // (World, Actor, GreenfootImage, Greenfoot and MouseInfo)

/**
 * Write a description of class Pacworld here.
 * 
 * @author (your name) 
 * @version (a version number or a date)
 */
public class Pacworld  extends World
{
    private Counter currentScore;
    public boolean canEatGhosts = false;
    /**
     * Constructor for objects of class Pacworld.
     * 
     */
    public Pacworld()
    {    
        // Create a new world with 600x400 cells with a cell size of 1x1 pixels.
        super(95, 125, 4);
        
        Greenfoot.setSpeed(80);
        
        // Add texts
        currentScore = new Counter();
        addObject(currentScore, 87, 7);
        
        Counter textScore = new Counter("Score:");
        addObject(textScore, 87, 2);
        
        // Add Pacman
        Pacman pacman = new Pacman();
        addObject(pacman, 48, 93);
        
        prepare();
        
        GreenfootSound start = new GreenfootSound("start.wav");
        start.play();
    }
    
    public Counter getCounter() {
        return currentScore;
    }
    
    public void prepare() {
        
    }

